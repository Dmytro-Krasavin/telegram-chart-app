{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["plugin:react/recommended", "airbnb-base/legacy"],
  "rules": {
    "class-methods-use-this": "off",
    "max-len": ["error", 140]
  },
  "parser": "babel-eslint"
}
